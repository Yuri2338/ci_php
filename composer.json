{
    "name": "yuri2338/get-remainder",
    "description": "Function that returns the remainder of the first number divided by the second",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RemainderCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^1.0",
        "rector/rector": "^0.17.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest",
        "lint": "pint",
        "analyze": "rector process --dry-run"
    }
}